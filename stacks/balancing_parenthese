#include <bits/stdc++.h>
using namespace std;
int check(string p){
	stack<char> s;
	int n=p.size();
	for(int i=0; i<n;i++){
		if(p[i]=='[' || p[i]=='{' || p[i]=='('){
			s.push(p[i]);
		}
		else if(p[i]==']'){					if(s.size()!=0)
{
			if(s.top()=='['){
				s.pop();
			}
else{return 0;}
		}else return 0;}
		else if(p[i]=='}'){					if(s.size()!=0)
{
			if(s.top()=='{'){
				s.pop();
			}else{return 0;}
		}else return 0;}
		else if(p[i]==')'){					if(s.size()!=0)
{
			if(s.top()=='('){
				s.pop();
			}else{return 0;}
		}else return 0;}
	}
	if(s.size()==0){
		return 1;
	}
	else return 0;
}
int main(){
	cout<<check("((((([{()}[]]]{{{[]}}})))))");
	return 0;
	
}
